<application>
  <component name="CodeInsightSettings">
    <option name="selectAutopopupSuggestionsByChars" value="true" />
    <option name="AUTO_POPUP_JAVADOC_INFO" value="true" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="SELECT_AUTOPOPUP_SUGGESTIONS_BY_CHARS" value="true" />
    <option name="AUTOCOMPLETE_ON_CODE_COMPLETION" value="false" />
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="15" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="C#" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="STRIP_TRAILING_SPACES" value="None" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern icon="question" case-sensitive="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;MARK - )(\W|$)(.*)" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;BUG - )(\W|$)(.*)">
      <option name="FOREGROUND" value="ba574e" />
      <option name="FONT_TYPE" value="3" />
      <option name="ERROR_STRIPE_COLOR" value="dc635b" />
    </pattern>
    <pattern case-sensitive="true" pattern="(?&lt;=\W|^)(?&lt;TAG&gt;TODO - )(\W|$)(.*)" />
  </component>
</application>